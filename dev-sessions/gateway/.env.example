# Copy this file to dev-sessions/gateway/.env and edit the values as needed.
# All paths must be absolute paths on the host machine.

SSH_USER=your-macos-username
SSH_HOST=host.docker.internal
SSH_PORT=22
DEV_SESSIONS_GATEWAY_PORT=6767
MAX_SESSIONS_PER_CREATOR=10
CLAUDE_GATEWAY_SSH_KEY_PATH=/Users/you/.ssh/claude_gateway
DEV_SESSIONS_DB_PATH=/Users/you/dev-sessions-gateway.db
DEV_SESSIONS_GATEWAY_CONTAINER_NAME=dev-sessions-gateway
